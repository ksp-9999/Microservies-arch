server:
  port: 8888
spring:
  application:
    name: ApiGateway
    
  cloud:
    gateway:
      routes:
      - id: welcomeapi
        uri: lb://WELCOMEAPI
        predicates:
        - Path: /welcome
        
      - id: greetapi
        uri: lb://GREET
        predicates:
        - Path: /greet

          
